{
  "name": "simple-electron-nfc",
  "version": "1.0.0",
  "main": "lib/main.js",
  "scripts": {
    "compile": "babel -d lib/ electron/",
    "dev": "npm run react-scripts start",
    "build": "react-scripts build",
    "electron": "electron .",
    "start": "concurrently \"cross-env BROWSER=none yarn react-scripts start\" \"npm run compile\" \"wait-on http://localhost:3000 && electron .\"",
    "pack": "electron-builder --dir",
    "dist": "npx build --x64 --macos --win --linux --c.extraMetadata.main=build/electron.js -p always"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "concurrently": "^4.0.1",
    "cross-env": "^5.2.0",
    "electron": "^3.0.7",
    "electron-builder": "^20.31.2",
    "electron-rebuild": "^1.8.2",
    "react-scripts": "^2.1.1",
    "version-bump-prompt": "^4.2.1",
    "wait-on": "^3.1.0"
  },
  "dependencies": {
    "electron-devtools-installer": "^2.2.4",
    "electron-is-dev": "^1.0.1",
    "electron-publisher-s3": "^20.17.2",
    "nfc-pcsc": "^0.6.2",
    "react": "^16.6.0",
    "react-dev-utils": "^6.1.1",
    "react-dom": "^16.6.0"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
